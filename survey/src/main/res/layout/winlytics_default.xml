<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/winlytics_scroll"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="match_parent"
            android:id="@+id/winlytics_customer_logo"
            android:layout_height="40dp" />
        <TextView
            android:layout_width="match_parent"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            android:layout_marginBottom="8dp"
            android:gravity="center"
            android:text="How likely are you recommend Apple to your friends?"
            android:id="@+id/winlytics_text_1"
            android:textColor="#000000"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="match_parent"
            android:text="VERY LIKELY"
            android:gravity="center"
            android:layout_marginBottom="10dp"
            android:id="@+id/winlytics_text_very_likely"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="10"
            android:tag="10"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_10"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="9"
            android:tag="9"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_9"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="8"
            android:tag="8"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_8"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="7"
            android:tag="7"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_7"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="6"
            android:tag="6"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_6"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="5"
            android:tag="5"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_5"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="4"
            android:tag="4"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_4"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="3"
            android:tag="3"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_3"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="2"
            android:tag="2"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_2"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="1"
            android:tag="1"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_1"
            android:layout_height="wrap_content" />
        <Button
            android:layout_width="match_parent"
            android:text="0"
            android:tag="0"
            android:textColor="#000000"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rounded_button_background_gray_onlyborder"
            android:layout_marginBottom="8dp"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:id="@+id/winlytics_button_0"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_width="match_parent"
            android:text="NOT LIKELY"
            android:gravity="center"
            android:layout_marginBottom="8dp"
            android:id="@+id/winlytics_text_not_likely"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:visibility="gone"
            android:id="@+id/winlytics_optional_text_area"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:background="#f4f3f3"
                android:gravity="center"
                android:textColor="#000000"
                android:id="@+id/winlytics_welcoming_text"
                android:paddingStart="20dp"
                android:paddingEnd="20dp"
                android:text="WE ARE HAPPY TO HEAR ASDASDASDAS DASSDASDA ASDDA"
                android:layout_height="100dp" />
            <TextView
                android:layout_width="match_parent"
                android:text="Tell Us Why?"
                android:textColor="#000000"
                android:layout_marginTop="25dp"
                android:layout_marginBottom="8dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:id="@+id/winlytics_optional_text_title_area"
                android:layout_height="wrap_content" />
            <EditText
                android:layout_width="match_parent"
                android:hint="asdasdasd"
                android:gravity="start"
                android:padding="8dp"
                android:layout_marginBottom="8dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/rounded_button_background_gray_onlyborder"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:id="@+id/winlytics_optional_edit_text_area"
                android:layout_height="150dp" />
            <Button
                android:layout_width="100dp"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:background="@drawable/rounded_button_background_gray_withborder"
                android:textColor="#FFFFFF"
                android:layout_gravity="center"
                android:id="@+id/winlytics_submit"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:text="Powered by Winlytics"
            android:layout_height="40dp" />
    </LinearLayout>
</ScrollView>